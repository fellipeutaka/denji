{
  "name": "denji",
  "description": "A CLI tool to manage your SVG icons",
  "type": "module",
  "private": false,
  "version": "0.2.0",
  "author": {
    "name": "Fellipe Utaka",
    "email": "fellipeutaka@gmail.com",
    "url": "https://github.com/fellipeutaka"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellipeutaka/denji.git",
    "directory": "apps/cli"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "configuration_schema.json",
    "README.md"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "start": "node .",
    "build": "bun run generate-schema && tsdown",
    "generate-schema": "bun run scripts/generate-schema.ts",
    "type-check": "tsc --noEmit --incremental false",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:doctor": "ultracite doctor"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "commander": "^14.0.3",
    "eta": "^4.5.0",
    "iconify-icon": "^3.0.2",
    "oxc-parser": "^0.112.0",
    "picocolors": "^1.1.1",
    "svgo": "^4.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@iconify/types": "^2.0.0",
    "tsdown": "^0.20.1"
  }
}
