{
  "name": "denji",
  "description": "A CLI tool to manage your SVG icons",
  "type": "module",
  "private": false,
  "version": "0.0.1",
  "author": {
    "name": "Fellipe Utaka",
    "email": "fellipeutaka@gmail.com",
    "url": "https://github.com/fellipeutaka"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fellipeutaka/denji.git"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "bin": "./dist/index.mjs",
  "files": [
    "dist",
    "denji.schema.json"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@iconify/types": "^2.0.0",
    "@types/bun": "^1.3.5",
    "@types/react": "^19.2.8",
    "lefthook": "^2.0.15",
    "tsdown": "^0.20.0-beta.1",
    "typescript": "^5.9.3",
    "ultracite": "7.0.11"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "start": "node .",
    "build": "bun run generate-schema && tsdown",
    "generate-schema": "bun run scripts/generate-schema.ts",
    "type-check": "tsc --noEmit --incremental false",
    "prepare": "lefthook install",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "lint:doctor": "ultracite doctor"
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "commander": "^14.0.2",
    "iconify-icon": "^3.0.2",
    "oxc-parser": "^0.108.0",
    "picocolors": "^1.1.1",
    "svgo": "^4.0.0",
    "zod": "^4.3.5"
  }
}
